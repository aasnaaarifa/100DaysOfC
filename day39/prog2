//Q78: Find the sum of main diagonal elements for a square matrix.

#include <stdio.h>
int main() {
    int n, sum = 0;
    printf("Enter number of rows and columns: ");
    scanf("%d %d", &n, &n);
	int matrix[n][n];
    for (int i = 0; i < n; i++) {
        for (int j = 0; j < n; j++) {
            printf("Enter element at position [%d][%d]: ", i + 1, j + 1);
            scanf("%d", &matrix[i][j]);
            if (i == j)
                sum += matrix[i][j];
        }
    }
	printf("\nSum of main diagonal elements = %d\n", sum);
	return 0;
}
