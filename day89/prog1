//Q139: Show that enums store integers by printing assigned values.

#include <stdio.h>
enum Status {
    SUCCESS,
    FAILURE,
    TIMEOUT
};
int main() {
    const char *names[] = {"SUCCESS", "FAILURE", "TIMEOUT"};
    printf("%s=%d, %s=%d, %s=%d\n",
           names[SUCCESS], SUCCESS,
           names[FAILURE], FAILURE,
           names[TIMEOUT], TIMEOUT);
    return 0;
}
