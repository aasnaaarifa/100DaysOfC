//Q134: Define an enum with SUCCESS, FAILURE, and TIMEOUT, and print messages accordingly.

#include <stdio.h>
#include <string.h>
#include <ctype.h>
enum Status { SUCCESS, FAILURE, TIMEOUT };
void toUpperStr(char *s) {
    for (int i = 0; s[i]; i++)
        s[i] = toupper(s[i]);
}
int main() {
    char input[20];
    enum Status st;
    printf("Enter status (SUCCESS, FAILURE, TIMEOUT): ");
    scanf("%s", input);
    toUpperStr(input);
    if (strcmp(input, "SUCCESS") == 0) st = SUCCESS;
    else if (strcmp(input, "FAILURE") == 0) st = FAILURE;
    else if (strcmp(input, "TIMEOUT") == 0) st = TIMEOUT;
    else {
        printf("Invalid status!\n");
        return 0;
    }
    switch (st) {
        case SUCCESS: printf("Operation successful\n"); break;
        case FAILURE: printf("Operation failed\n"); break;
        case TIMEOUT: printf("Operation timed out\n"); break;
    }
    return 0;
}
