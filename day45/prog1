//Q89: Count frequency of a given character in a string.

#include <stdio.h>
int main() {
    char str[1000], ch;
    printf("Enter a string: ");
    fgets(str, sizeof(str), stdin);
	printf("Enter the character to count: ");
    scanf("%c", &ch);
	int count = 0, i = 0;
    while (str[i] != '\0' && str[i] != '\n') {
        if (str[i] == ch)
            count++;
        i++;
    }
	printf("Frequency of '%c' = %d\n", ch, count);
    return 0;
}
